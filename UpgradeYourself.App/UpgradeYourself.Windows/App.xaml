<Application
    x:Class="UpgradeYourself.Windows.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UpgradeYourself.Windows"
    RequestedTheme="Dark">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <x:String x:Key="AppName">Upgrade Yourself</x:String>
                    <x:String x:Key="SubTitle">Improve your programming skills via daily training sessions</x:String>
                </ResourceDictionary>
                <ResourceDictionary Source="Styles/UpgradeYourselfStyles.xaml">
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
